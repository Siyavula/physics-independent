\battery[labeloffset=-.75](E)(A){$\mathcal{E}$}
\resistor[unit=0.5,dipolestyle=rectangle](E)(B){$r$}
%\psline(B)(C)
\resistor[dipolestyle=rectangle](B)(C){$R$}
\psline(D)(A)
