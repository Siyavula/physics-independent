\psline(A)(B)
\psline(C)(E)
\resistor[dipolestyle=rectangle](E)(F){$R_2=1 \, \Omega$}
\resistor[dipolestyle=rectangle](C)(D){$R_3=1 \, \Omega$}
\psline(D)(F)
\psline(J)(G)
\resistor[dipolestyle=rectangle,labelangle=90](G)(H){$R_4=1,5 \,  \Omega$}
\battery(I)(H){$V=10$ V}
</code>
